{
  "expo": {
    "name": "f1-app",
    "slug": "f1-app",
    "owner": "f1-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/f1.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.f1app.f1app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/f1.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.f1app.f1app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/f1.png",
          "imageWidth": 40,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-video",
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f461372a-a351-4f95-b405-75cf45d3652e"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f461372a-a351-4f95-b405-75cf45d3652e"
    }
  }
}
